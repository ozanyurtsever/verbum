    "@storybook/react": "^6.5.4",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/intl": "^1.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
